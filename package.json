{
  "name": "10x-gains",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration=development --host 0.0.0.0",
    "start:staging": "ng serve --configuration=staging",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:staging": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=reports/junit.xml",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:smoke": "cypress run --env grepTags=@smoke,grepFilterSpecs=true,grepOmitFiltered=true",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "copy-api-types": "node ./scripts/copy-api-types.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.7",
    "@angular/cdk": "19.2.9",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "19.2.9",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/postcss": "^4.1.3",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.3",
    "tslib": "^2.3.0",
    "zod": "^3.24.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.15.1",
    "@analogjs/vite-plugin-angular": "^1.15.1",
    "@analogjs/vitest-angular": "^1.17.1",
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/build": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@cypress/grep": "^4.1.0",
    "@nx/angular": "^20.7.2",
    "@nx/devkit": "^20.7.2",
    "@nx/vite": "^20.7.2",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "angular-eslint": "19.3.0",
    "cypress": "^14.4.1",
    "dotenv": "^16.5.0",
    "eslint": "8",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "jsdom": "^22.0.0",
    "lint-staged": "^15.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.27.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^3.2.2"
  },
  "lint-staged": {
    "src/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "supabase/functions/**/*.ts": [
      "deno lint --fix"
    ]
  }
}
